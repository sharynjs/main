{
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "start": "start-storybook -p 8025",
    "diff": "lerna diff",
    "updated": "babel-node build.js && lerna updated && babel-node build.js --clean",
    "pub": "yarn prepub && npm run justpub && yarn postpub",
    "prepub": "yarn clean && yarn lint && yarn test && babel-node build.js",
    "justpub": "lerna publish",
    "postpub": "babel-node build.js --clean",
    "build": "babel-node build.js",
    "clean": "babel-node build.js --clean",
    "lint": "markdownlint README.md docs packages -i node_modules -c .mdlintrc.json && eslint build.js babel.config.js packages && flow",
    "test": "yarn clean && babel-node build.js && cross-env USE_PROCESS_CWD_FOR_APP_ROOT=truthy jest",
    "focus-test": "jest packages/redux/src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn clean && yarn lint && yarn test"
    }
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "app-root-path": "^2.2.1",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-transform-flow-comments": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-backgrounds": "^4.0.0-alpha.21",
    "@storybook/addon-knobs": "^4.0.0-alpha.21",
    "@storybook/addon-viewport": "^4.0.0-alpha.21",
    "@storybook/addons": "^4.0.0-alpha.21",
    "@storybook/react": "^4.0.0-alpha.21",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-flow-react-proptypes": "^24.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.81.0",
    "flow-copy-source": "^2.0.2",
    "husky": "^1.0.0-rc.14",
    "jest": "^23.6.0",
    "lerna": "^3.4.3",
    "markdownlint": "^0.11.0",
    "markdownlint-cli": "^0.13.0",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.2",
    "rimraf": "^2.6.2",
    "storybook-addon-material-ui": "^0.9.0-alpha.18",
    "storybook-host": "^5.0.0"
  }
}
